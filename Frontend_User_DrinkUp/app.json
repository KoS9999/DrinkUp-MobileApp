{
  "expo": {
    "name": "Frontend_User_DrinkUp",
    "slug": "Frontend_User_DrinkUp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drinkup.frontend",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location for delivery services"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.drinkup.frontend",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Pacifico-Regular.ttf",
            "./assets/fonts/Oswald-Regular.ttf",
            "./assets/fonts/Oswald-Medium.ttf"
          ]
        }
      ]
    ]
  }
}